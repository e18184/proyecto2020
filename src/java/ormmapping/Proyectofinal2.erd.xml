  <View name="v_usuariorol" CreateStmt="create view gestion2019.v_usuariorol as  SELECT row_number() OVER () AS id,
    u.idusu,
    u.nombre AS unombre,
    u.apellido1,
    u.apellido2,
    r.nombre,
    d.login,
    d.password,
    d.activo
   FROM (gestion2019.usuarios u
     JOIN gestion2019.datos d ON ((u.idusu = d.idusu))),
    gestion2019.usurol ur,
    gestion2019.roles r
  WHERE ((u.idusu = ur.idusu) AND (ur.idrol = r.idrol));" schema="gestion2019" Materialized="false"/>
  <View name="v_listarestudiantes" CreateStmt="create view v_listarestudiantes as  SELECT row_number() OVER () AS id,
    uni.ru,
    usu.nombre,
    usu.apellido1,
    usu.apellido2
   FROM gestion2019.universitarios uni,
    gestion2019.usuarios usu
  WHERE (usu.idusu = uni.idusu);" schema="gestion2019" Materialized="false"/>
  <View name="v_listarproyectos" CreateStmt="create view v_listarproyectos as  SELECT row_number() OVER () AS id,
    usu.idusu AS usuario,
    uni.ru AS universitario,
    p.&quot;Titulo&quot;,
    p.&quot;Nota&quot;
   FROM gestion2019.&quot;Proyectos&quot; p,
    gestion2019.universitarios uni,
    gestion2019.programacion prog,
    gestion2019.usuarios usu
  WHERE ((p.programacionidprog = prog.idprog) AND (usu.idusu = prog.idusu) AND (usu.idusu = uni.idusu));" schema="gestion2019" Materialized="false"/>
